{
  "functions": {
    "api": {
      "handler": "app.app" // Esto asume que tu aplicación Flask se encuentra en un archivo llamado app.py y que la variable de aplicación se llama 'app'.
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/app.py" } // Esto redirige todas las solicitudes a tu archivo app.py
  ]
}
